{
  "name": "mcacheclean",
  "displayName": "M2cacheClean",
  "publisher": "mcacheclean",
  "icon": "images/logo.png",
  "author": "Rodrigo Cardoso da Luz",
  "license": "MIT",
  "description": "Extens√£o limpa as pastas var e generated na raiz de um projeto magento",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mcacheclean.clearcache"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mcacheclean.clearcache",
        "title": "Limpar Cache do Magento"
      }
    ],
    "keybindings": [
      {
        "command": "mcacheclean.clearcache",
        "key": "ctrl+alt+c",
        "mac": "cmd+alt+c"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diazrma/mcacheclean"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.58.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "generator-code": "^1.6.9"
  }
}
